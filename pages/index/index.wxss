/**index.wxss**/
.banner{
  width:100%;
  height: 260px;
}
.navtab{
  width:100%;
  display: flex;
  justify-content: space-around;
}
.navtab view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.navtab image{
  width:50px;
  height: 50px;
}
.tit{
  width:100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  margin-top:10px;
  padding:10px;
  font-size:14px;
}
.left{
  display: flex;
  align-items: center
}
.cf{
  width: 4px;
  height: 14px;
  border-radius:2px;
  background: #ff4d6a;
  display: block;
  margin-right:4px;
}
.bigSize{
  font-size:18px;
}
.smlSize{
  font-size: 12px;
  color:#ccc;
  margin-top:5px;
}
.swiper{
  width:100%;
  display: flex;
  padding:10px 0;
}
.swiper view{
  width:65%;
  flex-shrink: 0;
  display: flex;
  flex-direction: column;
}
.swiper view image{
  width:100%;
  height: 120px;
}
.content{
  padding-left:20px;
  box-sizing: border-box
}
